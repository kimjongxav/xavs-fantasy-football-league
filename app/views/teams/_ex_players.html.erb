<% @team.ex_players.each do |player| %>
<% gameweek_in = @team.player_team_relationships.where(:player_id => player.id).first.gameweek_in %>
<% gameweek_out = @team.player_team_relationships.where(:player_id => player.id).first.gameweek_out %>
  <tr>
    <td><%= link_to player.common_name, player %></td>
    <td><%= player.position %></td>
    <td><%= link_to player.premier_league_team.name, player.premier_league_team %></td>
    <td><%= gameweek_in %></td>
    <td><%= gameweek_out %></td>
    <td><%= player.gameweek_points.select { |k| k >= gameweek_in && k < gameweek_out }.values.sum %></td>
  </tr>
<% end %>
